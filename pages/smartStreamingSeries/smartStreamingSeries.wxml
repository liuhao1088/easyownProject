<import src="/template/myTemplate.wxml"></import>
<view class="container">
  <view class="tag_maxdiv" style="width: 100%;">
      <image mode="widthFix"
        src="https://img14.360buyimg.com/ddimg/jfs/t1/154715/39/1969/693313/5f83ff59E0efe6b23/496e458260ffb93a.png">
      </image>
  </view>


  <view class="tag_maxdiv" style="margin-top: 60rpx;">
